<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.myhome.entity">
    <class name="Version" table="t_version" dynamic-update="true">

        <id name="id" column="id" type="long">
            <generator class="native" />
        </id>

        <property name="invalid" column="invalid" type="boolean" />
        
        <property name="status" column="status" type="integer" />
        
        <property name="createdDatetime" column="created_datetime" type="timestamp" />
        
        <property name="updatedDatetime" column="updated_datetime" type="timestamp" />
        
        <property name="orderTag" column="order_tag" type="integer" />

        <property name="content" column="content" type="string" />
        
        <property name="versions" column="versions" type="string" />
        
        <property name="serviceAddress" column="serviceAddress" type="string" />
        
        <property name="size" column="size" type="string" />

    </class>
</hibernate-mapping>
